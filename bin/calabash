#!/usr/bin/env ruby
# TODO: Remove this
$LOAD_PATH.unshift File.join(File.dirname(__FILE__), '..', 'lib')

module Calabash
  module CLI
    require File.join(File.dirname(__FILE__), 'calabash_helpers')

    class CLI
      include Calabash::CLI::Helpers

      def initialize(arguments)
        @arguments = arguments
      end
    end
  end
end

Calabash::CLI::CLI.new(ARGV.dup)
